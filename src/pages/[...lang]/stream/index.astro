---
import {supportedLang} from "../../../lib/utils/languageParser";
import BaseLayoutStream from "./BaseLayoutStream.astro";
import RecentEpisodes from "./RecentEpisodes.astro";

export function getStaticPaths() {
  const paths = supportedLang.map((lang) => ({
  params: { lang: lang || undefined },
  }));
  return paths;
  }
---
<BaseLayoutStream>
  <RecentEpisodes />
</BaseLayoutStream>
